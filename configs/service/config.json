{
  "allowedRoles": {
    "admin": "${{ADMIN_ROLE_ID}}"
  },
  "publicCommands": [
    "myprofile",
    "skills"
  ],
  "channels": {
    "logChannel": "${{LOG_CHANNEL_ID}}",
    "welcomeChannel": "${{WELCOME_ROLE_ID}}",
    "roleSelectionChannel": "${{ROLE_SELECTION_CHANNEL_ID}}",
    "registrationChannel": "${{REGISTRATION_CHANNEL}}"
  },
  "registrations": {
    "autoInvite": true,
    "title": "New Registration",
    "inviteEmoji": "üìß",
    "email": [ "email", "Email Address" ]
  },	
  "clientId": "${{CLIENT_ID}}",
  "dataPartner": "notion",
  "emailPartner": "google",
	"guildId": "${{GUILD_ID}}",
  "logging": {
    "console": true,
    "discord": true
  },
  "partnerConfig": {
    "notion": {
      "keymappings": {
        "Discord Nickname": "nickname",
        "Discord Id": "id",
        "Discord Tag": "tag",
        "Groups": "groups",
        "Discord Invite": "invite",
        "Status": "Status"
      },
      "skills": [
        "Programming Languages",
        "Software Frameworks",
        "Additional Tooling",
        "Data Technologies",
        "Cloud Technologies"
      ],
      "request": {
        "baseURL": "https://api.notion.com/v1",
        "registationDatabase": "${{DATABASE_ID}}",
        "skillDatabase": "${{DATABASE_ID}}",
        "headers": { 
          "Notion-Version": "2022-06-28",
          "Authorization": "Bearer ${{SECRET}}",
          "Content-Type": "application/json"
        } 
      }
    },
    "google": {
      "credentials": {
        "client_id": "{{CLIENT_ID}",
        "project_id": "{{PROJECT_ID}}",
        "auth_uri": "https://accounts.google.com/o/oauth2/auth",
        "token_uri": "https://oauth2.googleapis.com/token",
        "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
        "client_secret": "{{CLIENT_SECRET}}",
        "redirect_uris": [
          "http://localhost"
        ],
        "tokens": {
          "note": "Generate with generateTokenWithOneTimeUseGoogleCode.js and put JSON here"
        }
      },
      "email": {
        "defaults": {
          "replyTo": "info@itsociety.org",
          "from": "IT Society<info@itsociety.org>",
          "headers": [
            { "key": "X-Application-Developer", "value": "Tyson Turkoz" },
            { "key": "X-Application-Version", "value": "v1.0.0" }
          ],
          "text": "This email is sent by IT Society Discord Server",
          "textEncoding": "base64"
        },
        "templates": {
          "welcomeEmail": "welcomeEmail.html",
          "reInviteEmail": "reInviteEmail.html"
        },
        "domain": "itsociety.org"
      }
    }
  },
  "roles": {
    "all": "all",
    "ü§≥": "ios-thrive",
    "ü§©": "qa-sdet",
    "ü§†": "tech-talks",
    "ü§ì": "system-design",
    "üò≠": "leetcode-medium-hard",
    "üìñ": "book-club",
    "üëΩ": "cyber-sec",
    "üë∂": "practice-programming-beginner",
    "üìè": "data-engineer",
    "üß°": "eng-management"
  },
  "startUpCorrelatation": false,
  "syncPeriod": 300000,
  "token": "${{TOKEN}}"
}